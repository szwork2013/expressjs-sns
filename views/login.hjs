<form action="/login" method="post" class="login-form">
    <div class="row">
        <label for="">邮箱</label> 
        <input type="text" name="username" placeholder="邮箱" />
    </div>
    <div class="row">
        <label for="">密码</label> 
        <input type="password" name="userpwd"/>
    </div>
    <div class="row">
        <div class="act">
            <button type="submit">登录</button>
        </div>
    </div>
</form>
